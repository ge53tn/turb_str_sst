FoamFile
{
    version      2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0]; // turbulent kinetic energy dissipation rate in m^2/s^3

internalField   uniform 0.00022813802409849448;

boundaryField
{
    freeSurface
    {
        type            epsilonFreeSurface;
		up				(0 0 1);
		value           $internalField;
    }
    bedWall
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    leftSide
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    rightSide
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    inletWater
    {
        type            fixedValue;
        value           $internalField;
    }
    outletWater
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
